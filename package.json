{
  "name": "mav-web-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prepare": "rm -drf ./.husky && npm run prepare-husky",
    "prepare-husky": "husky install && husky add .husky/pre-commit \"npx lint-staged\"",
    "prettier": "prettier --write \"(src|public)/**/*.+(ts|tsx|js|jsx|css|html|md|json)\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@chakra-ui/react": "^1.7.4",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@reduxjs/toolkit": "^1.7.1",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.11.19",
    "@types/react": "^17.0.38",
    "@types/react-datepicker": "^4.3.4",
    "@types/react-dom": "^17.0.11",
    "chakra-react-select": "^3.0.0",
    "dayjs": "^1.11.0",
    "firebase": "^9.9.3",
    "framer-motion": "^5.5.7",
    "html-react-parser": "^1.4.8",
    "immer": "^9.0.12",
    "phosphor-react": "^1.4.0",
    "react": "^17.0.2",
    "react-datepicker": "^4.7.0",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.0",
    "redux-logger": "^3.0.6",
    "typescript": "^4.5.4",
    "uuid": "^8.3.2",
    "web-vitals": "^2.1.3",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/redux-logger": "^3.0.9",
    "@types/uuid": "^8.3.4",
    "husky": "^7.0.4",
    "lint-staged": "^12.2.0",
    "prettier": "^2.5.1"
  }
}
